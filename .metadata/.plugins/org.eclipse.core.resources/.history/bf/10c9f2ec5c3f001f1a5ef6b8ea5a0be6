package com.kh.ch06_objectArray;

public class ObjectArrayRun {
	
	public static void main (String[] args) {
		/*
		 * 객체 배열 : 객체를 저장하는배열
		 *           배열의 각 요소가 참조변수들로 이루어짐
		 *           => 객체를 사용하려면 각 요소마다 객체 생성 필요
		 * 클래스명 [] 참조변수명(배열명) = new 클래스명[배열크기];
		 * => 배열 크기만큼 참조변수 배열 생성
		 * 
		 * 배열명[인덱스] = new 클래스명();//new 생성자;
		 * 
		 */
	}

}
